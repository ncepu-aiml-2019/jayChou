   "source": [
    "n = int(input('请输入求解的范围：'))\n",
    "\n",
    "numbers = [1 for i in range(n)]\n",
    "\n",
    "for i in range(2, n):\n",
    "    if numbers[i] != 0:\n",
    "        j = 2\n",
    "        while (j * i) < len(numbers):\n",
    "            numbers[i*j] = 0\n",
    "            j = j + 1\n",
    "\n",
    "for i in range(2, n):\n",
    "    if numbers[i] == 1:\n",
    "        print(i)\n",
    "\n"
   ]
  }
